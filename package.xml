<?xml version="1.0" encoding="UTF-8"?>
<package name="com.uz.jcoins.invitation" xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/package.xsd">
	<packageinformation>
		<packagename>JCoins - Invitation Extension</packagename>
		<packagedescription>Adds JCoins for invitations.</packagedescription>
		<packagename language="de"><![CDATA[JCoins - Einladung-Erweiterung]]></packagename>
		<packagedescription language="de">Ergänzt JCoins für Einladungen.</packagedescription>
		<version>5.5.0</version>
		<date>2022-05-27</date>
	</packageinformation>
	
	<authorinformation>
		<author>Zaydowicz></author>
	</authorinformation>
	
	<requiredpackages>
		<requiredpackage minversion="5.3.0">com.woltlab.wcf</requiredpackage>
		<requiredpackage minversion="5.3.0">com.uz.wcf.invitation</requiredpackage>
		<requiredpackage minversion="5.3.0">de.wcflabs.wcf.jcoins</requiredpackage>
	</requiredpackages>
	
	<excludedpackages>
		<excludedpackage version="5.6.0 Alpha 1">com.woltlab.wcf</excludedpackage>
	</excludedpackages>
	
	<instructions type="install">
		<instruction type="file">files.tar</instruction>
		<instruction type="eventListener">eventListener.xml</instruction>
		<instruction type="objectType">objectType.xml</instruction>
		<instruction type="language">language/*.xml</instruction>
	</instructions>
	
	<instructions type="update" fromversion="5.2.*">
		<instruction type="file">files.tar</instruction>
	</instructions>
	
	<instructions type="update" fromversion="5.3.*">
		<instruction type="file">files.tar</instruction>
	</instructions>
	
	<instructions type="update" fromversion="5.4.*">
		<instruction type="file">files.tar</instruction>
	</instructions>
</package>
